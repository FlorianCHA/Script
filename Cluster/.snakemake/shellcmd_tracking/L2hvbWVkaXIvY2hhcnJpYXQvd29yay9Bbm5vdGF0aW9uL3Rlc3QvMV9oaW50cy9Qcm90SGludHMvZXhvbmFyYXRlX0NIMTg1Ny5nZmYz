
	module load bioinfo/exonerate/2.4.7
	module load bioinfo/braker/1.9
	exonerate --model protein2genome --percent 95 --showtargetgff T /homedir/charriat/BGPI/becphy/pangenome2017/test70-15/exonerate/70-15_annotated_protein.fa /homedir/charriat/work/Annotation/test_input/CH1857_scaffold.fasta > /homedir/charriat/work/Annotation/test/1_hints/ProtHints/exonarate_CH1857.gff3
