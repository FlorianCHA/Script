
	module load bioinfo/exonerate/2.4.7
	module load bioinfo/braker/1.9
	exonerate --model protein2genome --percent 95 --showtargetgff T /work/gladieux/Script/Cluster/Annotation_pipeline_v2/SupplementaryFile/OG.fasta /work/gladieux/farman_genomes/3_masked/1_final-data/Bm8309.fasta > /work/gladieux/farman_genomes/4_Annotation/1_hints/ProtHints/exonerate_Bm8309.gff3
	exonerate2hints.pl --source=M --minintronlen=10 --maxintronlen=1000 --in=/work/gladieux/farman_genomes/4_Annotation/1_hints/ProtHints/exonerate_Bm8309.gff3 --out=/work/gladieux/farman_genomes/4_Annotation/1_hints/ProtHints/exonerate_Bm8309.hints.gff3
	