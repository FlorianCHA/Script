
	module load bioinfo/exonerate/2.4.7
	module load bioinfo/braker/1.9
	exonerate --model protein2genome --percent 95 --showtargetgff T /work/gladieux/Script/Cluster/Annotation_pipeline_v2/SupplementaryFile/OG.fasta /work/gladieux/farman_genomes/U168_merged.fasta > /work/gladieux/farman_genomes/1_tmp/1_hints/ProtHints/exonerate_U168_merged.gff3
